<div class="wrapper wrapper--w780">
    <div class="card card-3">
        <div class="card-heading"></div>
        <div class="card-body">
            {{ form_start(form) }}
                <div class="input-group input--style-3">
                    <div class='input'>
                        {{ form_widget(form) }}
                    </div> 
                </div>
                <div class="p-t-10 btn btn--pill btn--green">
                    <button class="btn">{{ button_label|default('Ajouter') }}</button>
                </div>
            {% if msg %}
                <div class="alert alert-success">
                    {{msg}}
                </div>
            {% endif %}
            {{ form_end(form) }}
            <br>
            Projet de l'Ã©quipe :
            {% for projet in projectInTeam %}
                <li>{{ projet.nameProject }}</li>
            {% endfor %}
        </div>
    </div>
</div>